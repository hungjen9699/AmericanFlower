<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>TempaServlet</servlet-name>
        <servlet-class>DemoServlet.TempaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>DemoServlet.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>DemoServlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvaluateStateServlet</servlet-name>
        <servlet-class>DemoServlet.EvaluateStateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForwardStateServlet</servlet-name>
        <servlet-class>DemoServlet.ForwardStateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForwardFlowerServlet</servlet-name>
        <servlet-class>DemoServlet.ForwardFlowerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvaluateFlowerServlet</servlet-name>
        <servlet-class>DemoServlet.EvaluateFlowerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForwardHomeServlet</servlet-name>
        <servlet-class>DemoServlet.ForwardHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>DemoServlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>DemoServlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreatePDFServlet</servlet-name>
        <servlet-class>DemoServlet.CreatePDFServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TempaServlet</servlet-name>
        <url-pattern>/TempaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/SearchController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EvaluateStateServlet</servlet-name>
        <url-pattern>/EvaluateStateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForwardStateServlet</servlet-name>
        <url-pattern>/ForwardStateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForwardFlowerServlet</servlet-name>
        <url-pattern>/ForwardFlowerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EvaluateFlowerServlet</servlet-name>
        <url-pattern>/EvaluateFlowerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForwardHomeServlet</servlet-name>
        <url-pattern>/ForwardHomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreatePDFServlet</servlet-name>
        <url-pattern>/CreatePDFServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
